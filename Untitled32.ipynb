    "def pal(txt):\n",
    "    l=0\n",
    "    r=len(txt)-1\n",
    "    while(l<r):\n",
    "        if txt[l]!=txt[r]:\n",
    "            return False\n",
    "        else:\n",
    "            l,r=l+1,r-1\n",
    "    return True\n",
    "pal(\"malayalam\")\n",
